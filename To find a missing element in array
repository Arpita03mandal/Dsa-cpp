#include<bits/stdc++.h>
using namespace std;

int missingNum(int arr[],int size) {
    int n=size+1;
    int total=n*(n+1)/2;
    int sum=0;
    for(int i=0;i<size;i++) {
        sum+=arr[i];
    }
    return total-sum;
}
int main() {
    int arr[]={1,2,4,5};
    int size=4;
    cout<<"Missing Number is "<<missingNum(arr,size);
    return 0;
}
