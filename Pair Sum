#include<bits/stdc++.h>
using namespace std;
int main()
{
    vector<int>nums={2,7,11,15};
    int target=13;
    int n=nums.size();
    for(int i=0;i<n;i++)
    {
        for(int j=i+1;j<n;j++) 
        {
            if(nums[i]+nums[j]==target)
            {
                nums.push_back(i);
                nums.push_back(j);
            }
        }
    }
    cout<<nums[0]<<","<<nums[2]<<endl;
    return 0;
}
