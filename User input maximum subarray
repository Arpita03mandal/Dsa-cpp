#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n=5;
    int arr[5];
    cout<<"Enter elements:";
    for(int i=0;i<n;i++) {
         cin>>arr[i];
    }
   
    for(int st=0;st<n;st++)
    
    {
        for(int end=st;end<n;end++)
        {
            for(int i=st;i<=end;i++)
            {
                cout<<arr[i]<<" ";
            }
        }
        cout<<"\t";
        cout<<endl;
    }
    return 0;
}
