#include<bits/stdc++.h>
using namespace std;

int main() {
    int arr[]={12, 35, 1, 10, 34, 1};
    int size=6;
    int largest=INT_MIN;
    int secondlargest=INT_MIN;
    for(int i=0;i<size;i++) {
        if(arr[i]>largest) {
            secondlargest=largest;
            largest=arr[i];
        }
        else if (arr[i]>secondlargest && arr[i]!=largest) {
            secondlargest=arr[i];
        }
    }
    cout<<"Second Largest Number is "<<secondlargest<<endl;
    return 0;
}
