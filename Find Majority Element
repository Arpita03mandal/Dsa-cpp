#include<bits/stdc++.h>
using namespace std;
int MajorityElement(vector<int>nums) 
{
    int n=nums.size();
    for(int val:nums)
    {
        int freq=0;
        for(int x:nums) 
        {
            if(x==val)
            freq++;
        }
        if(freq>n/2)
        return val;
    }
    return -1;
}
int main(){
    vector<int>nums={1,2,2,1,1};
    cout<<"majority Element="<<MajorityElement(nums);
    return 0;
}
