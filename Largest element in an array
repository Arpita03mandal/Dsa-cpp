#include<bits/stdc++.h>
using namespace std;

int main() 
{
    int size=5;
    int nums[]={34,22,55,45,87};
    int largest=INT_MIN;
    for(int i=0;i<=size;i++)
    {
        if(nums[i]>largest)
        {
            largest=nums[i];
        }
    }
    cout<<"Largest Number= "<<largest<<endl;
    return 0;
}
